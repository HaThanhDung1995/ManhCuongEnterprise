@model List<WebEnter.Models.Feedback>
@{
    ViewBag.Title = "ManagerFeedBack";
}

<h2>ManagerFeedBack</h2>

<div id="list" class="tab-pane">
    <div class="panel panel-success">
        <table class="table table-hover">
            <thead>
            <tr>
                <th>Id</th>
                <th>Student</th>
                <th>Tutor</th>
                <th>FeedBack</th>
                    
            </tr>
            </thead>
            <tbody>
            @foreach (var Item in Model)
            {
                <tr>
                    <td>@Item.Id</td>
                    <td>@Item.Student.Fullname</td>
                    <td>@Item.Turtor.Fullname</td>
                    <td>@Item.Feedback1</td>
                </tr>
            }
            </tbody>
        </table>
    </div>
</div>
@Model.Count()